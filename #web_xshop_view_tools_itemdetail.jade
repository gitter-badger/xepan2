  script(type='text/javascript').
    $("#jRate_378").jRate({
    rating: '',
    shape: "STAR",
    count: 5,
    width: "20",
    height: "20",
    widthGrowth: 0.0,
    heightGrowth: 0.0,
    backgroundColor: "gray",
    startColor: "yellow",
    endColor: "green",
    strokeColor: "black",
    transparency: 1,
    shapeGap: "0px",
    opacity: 1,
    min: 0,
    max: 5,
    precision: 1,
    minSelected: 0,
    strokeWidth: '2px',
    horizontal: true,
    reverse: false,
    readOnly: false,
    touch: true,
    onChange: function(rating) {
    $('#jRate_378-value').text("Your Rating: "+rating);
    },
    onSet:function(rating){
    url = $('#jRate_378').attr('subpage');
    item_id = $('#jRate_378').attr('xitem');
    days = $('#jRate_378').attr('xdays');
    $(this).closest('.xshop-item-detail').univ().reload('index.php?subpage='+url,{'review':true,'rating':rating,'xsnb_item_id':item_id,'days':days,'cut_object':$(this).closest('.xshop-item-detail').attr('id')});
    }
    });
