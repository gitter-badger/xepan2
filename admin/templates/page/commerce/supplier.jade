  body
    .main-box.no-header.clearfix
      header.main-box-header.clearfix
        h2.pull-left Supplier
        .filter-block.pull-right
          .form-group.pull-left
            input.form-control(type='text', placeholder='Search...')
            i.fa.fa-search.search-icon
          a.btn.btn-primary.pull-right(href='#')
            i.fa.fa-plus-circle.fa-lg
            |  Add Supplier
          .btn-group.pull-right
            button.btn.btn-primary Category Filter
            button.btn.btn-primary.dropdown-toggle(data-toggle='dropdown', data-placeholder='false')
              span.caret
            ul.dropdown-menu
              li
                input#ID(type='checkbox', name='NAME', value='VALUE')
                label(for='ID') Jaipur
              li
                input#ID(type='checkbox', name='NAME', value='VALUE')
                label(for='ID') Udaipur
              li
                input#ID(type='checkbox', name='NAME', value='VALUE')
                label(for='ID') Jodhpur
          .form-group.pull-right
            button.btn.btn-primary.dropdown-toggle(type='button', data-toggle='dropdown')
              | Sort By
              span.caret
            ul.dropdown-menu
              li
                label(for='ID') Date Created
              li
                label(for='ID') Activness
              li
                label(for='ID') Source
